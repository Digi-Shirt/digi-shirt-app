{
  "expo": {
    "name": "Digi-Shirt",
    "slug": "DigiShirt",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.9",
    "orientation": "portrait",
    "icon": "./assets/images/digi-shirt-icon.png",
    "scheme": "digishirt",
    "splash": {
      "image": "./assets/images/digi-shirt-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.digishirt.digishirtapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.bulldogonaskateboard.digishirt",
      "versionCode": 1
    },
    "description": "Product demo of Digi-Shirt.",
    "githubUrl": "https://github.com/Digi-Shirt/digi-shirt-app"
  }
}
